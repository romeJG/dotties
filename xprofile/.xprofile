xset s off
xset -dpms
xset s noblank
(sleep 6 && picom --experimental-backends --config ~/.config/picom/picom.conf) &
xinput set-prop 11 "libinput Natural Scrolling Enabled" 1
xinput set-prop 11 "libinput Tapping Enabled" 1
export GTK_THEME="Layan-Dark-Solid"
greenclip daemon &
export XCURSOR_THEME=material_light_cursors
export XCURSOR_SIZE=24  
